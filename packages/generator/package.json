{
  "name": "prisma-generator-hasura-compat",
  "description": "Provide a description about your generator",
  "version": "1.0.0",
  "main": "dist/generator.js",
  "license": "MIT",
  "bin": "dist/bin.js",
  "engines": {
    "node": ">=14.0",
    "yarn": "3.6.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:jspears/prisma-generator-hasura-compat.git"
  },
  "scripts": {
    "start": "node dist/bin.js",
    "dev": "tsc -w",
    "build": "tsc",
    "prepack": "yarn build",
    "test": "jest"
  },
  "dependencies": {
    "@prisma/client": "5.2.0",
    "@prisma/generator-helper": "5.2.0",
    "@prisma/internals": "5.2.0",
    "graphql": "^16.8.1"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@types/jest": "27.0.3",
    "@types/node": "17.0.21",
    "@types/prettier": "2.4.2",
    "jest": "27.4.7",
    "prisma": "5.7.1",
    "semantic-release": "^18.0.1",
    "ts-jest": "27.1.4",
    "typescript": "^5.0.0"
  },
  "homepage": "Link to homepage or github readme here",
  "author": "Your Name <Your Email>",
  "keywords": [
    "prisma",
    "prisma2",
    "generator"
  ],
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md"
        }
      ],
      [
        "@semantic-release/npm",
        {
          "pkgRoot": "dist"
        }
      ],
      "@semantic-release/github",
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "package.json"
          ],
          "message": "chore(release): set `package.json` to ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  }
}
